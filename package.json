{
  "name": "napcon",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "bun@1.3.9",
  "description": "Lightweight NapCat/Onebot v11 TypeScript library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suisanka/napcon.git"
  },
  "keywords": [
    "onebot",
    "napcat",
    "sdk",
    "qq",
    "qq-robot",
    "typescript",
    "onebot11",
    "cqhttp"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.mts",
      "import": "./dist/utils/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  },
  "scripts": {
    "format": "eslint --fix .",
    "build": "tsdown",
    "bump": "bumpp"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@types/bun": "latest",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "jiti": "^2.6.1",
    "tsdown": "^0.20.3"
  }
}
